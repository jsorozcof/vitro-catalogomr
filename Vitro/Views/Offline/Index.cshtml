@{
    ViewBag.Title = "Vitro Offline";
}

<h2 class="h1 text-upper">@ViewBag.Title</h2>

<div class="row">
    <div class="cell-md-6">
        @using (Html.BeginForm("Create", "Offline", FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            <div class="form-group">
                <button type="submit" class="button fg-white" style="background-color: #0057b8;">GENERAR</button>
            </div>
        }
    </div>
</div>
@section Scripts{
    @Scripts.Render("~/bundles/jqueryval")
}